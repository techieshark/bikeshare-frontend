{
  "name": "bikeshare-frontend",
  "version": "1.0.0",
  "description": "Bike share frontend",
  "main": "app.js",
  "repository": "git@github.com:techieshark/bikeshare-frontend.git",
  "author": "Peter W <techieshark@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.4.1",
    "@mapbox/polyline": "^1.0.0",
    "@turf/buffer": "^4.7.4",
    "@turf/circle": "^5.0.0",
    "@turf/helpers": "^5.0.1",
    "@turf/within": "^4.7.3",
    "autocompleter": "^1.0.29",
    "nouislider": "^10.1.0"
  },
  "scripts": {
    "build": "webpack && cp src/index.html dist/",
    "build-min": "webpack --optimize-minimize && cp src/index.html dist/",
    "deploy": "gh-pages -d dist",
    "watch": "webpack --progress --watch",
    "start": "cp src/index.html dist/ && webpack-dev-server --https --cert=ssl/cert.pem --key=ssl/key.pem --open",
    "start-prod": "cd dist && http-server -S -C ../ssl/cert.pem -K ../ssl/key.pem -p 8081",
    "server": "http-server -p 3000 --cors",
    "build-debug": "webpack --display-modules",
    "clean": "rm -rf dist/*",
    "lint": "./node_modules/.bin/eslint src/",
    "lintfix": "./node_modules/.bin/eslint --fix src/"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "file-loader": "^1.1.5",
    "gh-pages": "^2.0.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  }
}
